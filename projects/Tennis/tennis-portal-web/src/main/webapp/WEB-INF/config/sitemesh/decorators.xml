<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/decorators">
    <excludes>
        <pattern>/static/*</pattern>
        <pattern>*decorator=none*</pattern>
        <pattern>*.rss</pattern>
        <pattern>*.csv</pattern>
        <pattern>*.txt</pattern>
    </excludes>

    <decorator name="default" page="default-layout.st">
        <pattern>/*</pattern>
    </decorator>
</decorators>